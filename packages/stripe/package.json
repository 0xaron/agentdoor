{
  "name": "@agentgate/stripe",
  "version": "0.1.0",
  "description": "Stripe billing bridge for AgentGate - reconcile x402 payments as Stripe invoice items",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentgate/core": "workspace:*"
  },
  "peerDependencies": {
    "stripe": ">=14"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "@types/node": "^22.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentgate/agentgate",
    "directory": "packages/stripe"
  },
  "keywords": [
    "agentgate",
    "stripe",
    "billing",
    "x402",
    "payments"
  ]
}
