{
  "name": "@agentdoor/cli",
  "version": "0.1.0",
  "description": "CLI tool for AgentDoor - init, keygen, and status commands",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentdoor": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentdoor/core": "workspace:*",
    "commander": "^12.1.0",
    "inquirer": "^12.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "@types/node": "^22.0.0",
    "@types/inquirer": "^9.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentdoor/agentdoor",
    "directory": "packages/cli"
  },
  "keywords": [
    "agentdoor",
    "cli",
    "agent",
    "auth",
    "init",
    "keygen"
  ]
}
