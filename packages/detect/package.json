{
  "name": "@agentgate/detect",
  "version": "0.1.0",
  "description": "Agent traffic detection and fingerprinting for AgentGate",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./signals": {
      "types": "./dist/signals.d.ts",
      "import": "./dist/signals.js"
    },
    "./fingerprint": {
      "types": "./dist/fingerprint.d.ts",
      "import": "./dist/fingerprint.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentgate/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "@types/node": "^22.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentgate/agentgate",
    "directory": "packages/detect"
  },
  "keywords": [
    "agentgate",
    "detect",
    "agent",
    "bot",
    "fingerprint",
    "traffic",
    "classification"
  ]
}
